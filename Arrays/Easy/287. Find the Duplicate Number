T.C:O(N)
S.C:O(1)
---------------------
class Solution {
public:
    int findDuplicate(vector<int>& nums)
    {
      int slow,fast;
      slow = fast = nums[0];
      do
      {
        slow = nums[slow];
        fast = nums[nums[fast]];
      }while(slow!=fast);
      
      fast = nums[0];
      while(fast != slow)
      {
        fast = nums[fast];
        slow = nums[slow];
      }
       return slow; 
    }
};
