T.C:O(N)
S.C:O(N)
------------------------------
class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int>m;
        for(int i = 0; i < nums.size() ; i++)
        {
            int r = target - nums[i];
            if(m.find(r) != m.end())
            {
                nums.erase(nums.begin(),nums.end());
                nums = {i,m[r]};
                break;
            }
            m[nums[i]] = i;
        }
        return nums;
    }
};
